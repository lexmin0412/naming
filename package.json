{
	"name": "@lexmin0412/naming",
	"version": "0.0.1",
	"description": "naming utilities",
	"main": "./lib/index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build:commonjs": "rimraf lib && tsc --target es5 --outDir lib --module commonjs --declaration",
		"build:es": "rimraf es && tsc --target es6 --outDir es --module es6",
		"build": "npm run build:commonjs && npm run build:es",
		"ts": "ts-node src/",
		"prepublishOnly": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lexmin0412/naming.git"
	},
	"keywords": [
		"naming",
		"util",
		"case"
	],
	"author": "lexmin0412 <zhangle_dev@outlook.com>",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/lexmin0412/naming/issues"
	},
	"homepage": "https://github.com/lexmin0412/naming#readme",
	"devDependencies": {
		"@types/node": "^18.16.0",
		"rimraf": "^5.0.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	},
	"files": [
		"es",
		"lib"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"types": "./lib/index.d.ts",
	"jsnext:main": "./es/index.js",
	"module": "./es/index.js"
}
